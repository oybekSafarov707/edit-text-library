<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CheckedPasswordValidator">
        <attr format="boolean" name="passwordEnabled"/>
        <attr format="integer" name="maxLength"/>
    </declare-styleable>
    <declare-styleable name="ValidatorEditText">
        <attr format="enum" name="typeInput">
            <enum name="text" value="0"/>
            <enum name="phone_number" value="1"/>
            <enum name="ip_address" value="2"/>
            <enum name="password" value="3"/>
            <enum name="email_address" value="4"/>
            <enum name="zip_code" value="5"/>
            <enum name="year" value="6"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="SpinnerValidator">
        <attr format="string" name="hint"/>
    </declare-styleable>
</resources>